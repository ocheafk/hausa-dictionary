@@ -0,0 +1,29 @@
igbo = igbo_dict = {
    "dog": "nkita",
     "cat": "pusi",
    "goat": "ewu",
    "cow": "ehi",
     "horse": "hai",
    "sheep": "aturu",
    "pig": "ezi" ,
    "chicken": "okuko",
    "rat": "oke",
    "fish": "azu",
    "hawk": "ugo",
    "snail": "ejula",
    "rabbit": "onyeokuku",
    "bat": "anwanta",
    "turkey": "tooki",
    "tortoise": "mbe",
    "snake": "agwo",
    "elephant": "enyi",
     "monkey": "enwe",
    "grasshopper": "ukpana",
}
key = input("Welcome to the igbo dictionary, Enter an animal you would like to translate to igbo ")
if key in igbo_dict:
    print(f"The igbo translation for {key} is {igbo_dict[key]}")
else:
    print('Sorry, the animal you are trying to translate is not in the dictionary')
